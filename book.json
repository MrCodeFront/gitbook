{
  "root": "./doc",
  "title": "CodeFront Document",
  "author": "lkf",
  "description": "",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "structure": {
    "readme": "README.md"
  },
  "plugins": [
    "-lunr",
    "-search",
    "search-plus",
    "expandable-chapters",
    "theme-comscore",
    "tbfed-pagefooter",
    "-highlight",
    "prism-themes",
    "code",
    "expandable-chapters-small",
    "github",
    "my-toolbar",
    "hide-element",
	"accordion"
  ],
  "pluginsConfig": {
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy 951450262@qq.com 2021",
      "modify_label": "文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "github": {
      "url": "https://github.com/MrCodeFront/my-gitbook"
    },
    "my-toolbar": {
      "buttons": [{
        "label": "下载PDF",
        "icon": "fa fa-file-pdf-o",
        "url": "http://localhost:4000/book.pdf",
        "position": "left",
        "text": "下载PDF",
        "target": "_blank"
      }]
    },
    "hide-element": {
      "elements": [
        ".gitbook-link"
      ]
    }
  },
  "styles": {
    "website": "./styles/website.css"
  }
}